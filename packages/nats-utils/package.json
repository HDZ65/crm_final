{
  "name": "@crm/nats-utils",
  "version": "1.0.0",
  "description": "NATS JetStream utilities for CRM microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
