# service-core - development Configuration
NODE_ENV=development

# Database (postgres-main, identity_db database)
DB_HOST=postgres-main
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=identity_db

# gRPC (single port for all protos: users, clients, documents)
GRPC_PORT=50052
HTTP_PORT=3052

# NATS
NATS_URL=nats://nats:4222

# JWT (for auth)
JWT_SECRET=your-256-bit-secret-key-here
JWT_EXPIRATION=3600
