# service-engagement - development Configuration
NODE_ENV=development

# Database (Instance globale partagée sur le serveur)
DB_HOST=global_postgres
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=alex_engagement  #

# gRPC / HTTP
GRPC_PORT=50051
HTTP_PORT=3061

# NATS (Ton instance personnelle sur le serveur)
NATS_URL=nats://alex-crm-nats:4222  #

# Redis (Instance globale partagée sur le serveur)
REDIS_HOST=global_redis  #
REDIS_PORT=6379

# Encryption & OAuth (Gardés tels quels pour ton dev)
ENCRYPTION_KEY=replace-with-32-byte-secret-key
ZOOM_CLIENT_ID=zoom-client-id
ZOOM_CLIENT_SECRET=zoom-client-secret
GOOGLE_CLIENT_ID=google-client-id
GOOGLE_CLIENT_SECRET=google-client-secret
MICROSOFT_CLIENT_ID=microsoft-client-id
MICROSOFT_CLIENT_SECRET=microsoft-client-secret