# service-finance - development Configuration
NODE_ENV=development

# Database (postgres-main, finance_db database)
DB_HOST=postgres-main
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=finance_db

# gRPC (single port for all protos: factures, payments, calendar)
GRPC_PORT=50059
HTTP_PORT=3059

# NATS
NATS_URL=nats://nats:4222

# JWT (for auth)
JWT_SECRET=your-256-bit-secret-key-here
JWT_EXPIRATION=3600
